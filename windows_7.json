{
  "provisioners": [
    {
      "type": "file",
      "source": "/prepare/resource/7z920-x64.msi",
      "destination": "C:/Windows/Temp/7z920-x64.msi"
    },
    {
      "type": "file",
      "source": "/prepare/resource/ultradefrag-portable-6.1.0.bin.amd64.zip",
      "destination": "C:/Windows/Temp/ultradefrag.zip"
    },
    {
      "type": "file",
      "source": "/prepare/resource/SDelete.zip",
      "destination": "C:/Windows/Temp/SDelete.zip"
    },
    {
      "execute_command": "{{.Vars}} cmd /c C:/Windows/Temp/script.bat",
      "remote_path": "/tmp/script.bat",
      "scripts": [
        "script/vagrant.bat",
        "script/vm-guest-tools.bat",
        "script/compact.bat"
      ],
      "type": "shell"
    },
    {
      "inline": [
        "rm -f /tmp/script.bat"
      ],
      "type": "shell"
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "keep_input_artifact": false,
      "output": "windows_7_{{.Provider}}.box",
      "vagrantfile_template": "vagrantfile-windows-7.tpl"
    }
  ],
  "builders": [
    {
      "boot_wait": "120s",
      "disk_size": 102400,
      "floppy_files": [
        "floppy/win7/Autounattend.xml",
        "floppy/install-winrm.cmd",
        "floppy/fixnetwork.ps1",
        "floppy/misc.bat",
        "floppy/disablewinupdate.bat",
        "floppy/openssh.bat",
        "floppy/zz-start-sshd.cmd",
        "floppy/oracle-cert.cer"
      ],
      "guest_os_type": "Windows7_64",
      "hard_drive_interface": "sata",
      "iso_checksum": "A491F985DCCFB5863F31B728DDDBEDB2FF4DF8D1",
      "iso_checksum_type": "sha1",
      "iso_url": "file:///prepare/resource/en_windows_7_enterprise_with_sp1_x64_dvd_u_677651.iso",
      "ssh_password": "vagrant",
      "ssh_username": "vagrant",
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "shutdown /s /t 10 /f /d p:4:1 /c \"Packer Shutdown\"",
      "type": "virtualbox-iso",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "2048"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ]
      ],
      "vm_name": "windows_7"
    },
    {
      "boot_wait": "120s",
      "disk_size": 102400,
      "floppy_files": [
        "floppy/win7/Autounattend.xml",
        "floppy/install-winrm.cmd",
        "floppy/fixnetwork.ps1",
        "floppy/misc.bat",
        "floppy/disablewinupdate.bat",
        "floppy/openssh.bat",
        "floppy/zz-start-sshd.cmd",
        "floppy/oracle-cert.cer"
      ],
      "guest_os_type": "win-7",
      "hard_drive_interface": "sata",
      "iso_checksum": "A491F985DCCFB5863F31B728DDDBEDB2FF4DF8D1",
      "iso_checksum_type": "sha1",
      "iso_url": "file:///prepare/resource/en_windows_7_enterprise_with_sp1_x64_dvd_u_677651.iso",
      "parallels_tools_flavor": "win",
      "prlctl": [
        [
          "set",
          "{{.Name}}",
          "--memsize",
          "2048"
        ],
        [
          "set",
          "{{.Name}}",
          "--cpus",
          "2"
        ]
      ],
      "ssh_password": "vagrant",
      "ssh_username": "vagrant",
      "ssh_wait_timeout": "10000s",
      "shutdown_command": "shutdown /s /t 10 /f /d p:4:1 /c \"Packer Shutdown\"",
      "type": "parallels-iso",
      "vm_name": "windows_7"
    }
  ]
}
